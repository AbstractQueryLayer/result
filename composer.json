{
    "name": "ifcastle/aql-result",
    "description": "Result container abstraction for AQL.",
    "keywords": [
        "aql",
        "query",
        "language",
        "storage"
    ],
    "type": "ifcastle-aql-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Edmond Dantes",
            "email": "edmondifthen@proton.me"
        }
    ],
    "require": {
        "php": ">=8.4",
        "ifcastle/di": "^0.6.0",
        "ifcastle/type-definitions": "^0.5.7",
        "ifcastle/exceptions": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "IfCastle\\AQL\\Result\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IfCastle\\AQL\\Result\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2",
        "rector/rector": "^1.2"
    },
    "scripts": {
        "test": "vendor/bin/phpunit ./tests"
    }
}